{
  "name": "env",
  "triggers": [
    "env",
    "printenv",
    "export"
  ],
  "rules": [
    {
      "name": "Path Variable",
      "pattern": "^(PATH|LD_LIBRARY_PATH|MANPATH)(=)(.*)$",
      "colors": [
        "bold cyan",
        null,
        "yellow"
      ],
      "enabled": true,
      "description": "Highlights PATH variables"
    },
    {
      "name": "Display Variables",
      "pattern": "^(DISPLAY|WAYLAND_DISPLAY|XDG_\\w+)(=)(.*)$",
      "colors": [
        "magenta",
        null,
        "yellow"
      ],
      "enabled": true,
      "description": "Highlights display-related variables"
    },
    {
      "name": "Language Variables",
      "pattern": "^(LANG|LC_\\w+|LANGUAGE)(=)(.*)$",
      "colors": [
        "magenta",
        null,
        "bright_blue"
      ],
      "enabled": true,
      "description": "Highlights locale variables"
    },
    {
      "name": "Home/User Variables",
      "pattern": "^(HOME|USER|LOGNAME|SHELL)(=)(.*)$",
      "colors": [
        "yellow",
        null,
        "green"
      ],
      "enabled": true,
      "description": "Highlights user-related variables"
    },
    {
      "name": "Variable Assignment",
      "pattern": "^([A-Z_][A-Z0-9_]*)(=)(.*)$",
      "colors": [
        "cyan",
        null,
        "yellow"
      ],
      "enabled": false,
      "description": "Highlights variable name and value"
    }
  ],
  "enabled": true,
  "description": "Highlights for env environment variables output",
  "use_global_rules": false
}
